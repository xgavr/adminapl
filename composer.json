    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Admin\\": "module/Admin/src/",
            "Company\\": "module/Company/src/",
            "User\\": "module/User/src/",
            "Bankapi\\": "module/Bankapi/src/",
            "Bank\\": "module/Bank/src/",
            "Stock\\": "module/Stock/src/",
            "ApiMarketPlace\\": "module/ApiMarketPlace/src/",
            "ApiSupplier\\": "module/ApiSupplier/src/",
            "Cash\\": "module/Cash/src/",
            "Api\\": "module/Api/src/",
            "ApiReactor\\": "module/ApiReactor/src/",
            "Ai\\": "module/Ai/src/",
            "Search\\": "module/Search/src/",
            "Fin\\": "module/Fin/src/",
            "Zp\\": "module/Zp/src/",
            "GoodMap\\": "module/GoodMap/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "AdminTest\\": "module/Admin/test/",
            "CompanyTest\\": "module/Company/test/",
            "UserTest\\": "module/User/test/",
            "BankapiTest\\": "module/Bankapi/test/",
            "BankTest\\": "module/Bank/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "laminas/laminas-developer-tools": "^2.9",
        "laminas-api-tools/api-tools-admin": "~2.1"
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-component-installer": true,
            "php-http/discovery": true
        }
    }
}
